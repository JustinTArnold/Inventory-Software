<DropDownMenu@DropDown>:
    Button:
        text: 'Home'
        size_hint_y: None
        height: 48
        background_down: 'orange.png'
        on_release: 
            app.sm.current = 'MainMenu'
            root.dismiss()

    Button:
        text: '--------------------------'
        size_hint_y: None
        color: 1, .5, 0, 1
        height: 48
        background_down: 'orange.png'
        on_release: 
            root.test()
            root.dismiss()

    Button:
        text: 'New Switch'
        size_hint_y: None
        height: 48
        background_down: 'orange.png'
        on_release: 
            app.sm.current = 'newswc'
            root.dismiss()

    Button:
        text: 'New Camera'
        size_hint_y: None
        height: 48
        background_down: 'orange.png'
        on_release: 
            app.sm.current = 'newcam'
            root.dismiss()

    Button:
        text: 'New Router'
        size_hint_y: None
        height: 48
        background_down: 'orange.png'
        on_release: 
            root.dismiss()
            app.sm.current = 'newrtr'

    Button:
        text: 'New DVR'
        size_hint_y: None
        height: 48
        background_down: 'orange.png'
        on_release: 
            app.sm.current = 'newdvr'
            root.dismiss()

    Button:
        text: 'New Phone'
        size_hint_y: None
        height: 48
        background_down: 'orange.png'
        on_release: 
            app.sm.current = 'newphn'
            root.dismiss()

    Button:
        text: 'New Insomniac'
        size_hint_y: None
        height: 48
        background_down: 'orange.png'
        on_release: 
            app.sm.current = 'newksk'
            root.dismiss()

    Button:
        text: '--------------------------'
        color: 1, .5, 0, 1
        size_hint_y: None
        height: 48
        background_down: 'orange.png'
        on_release: 
            root.test()
            root.dismiss()

    Button:
        text: 'Send to Field'
        size_hint_y: None
        height: 48
        background_down: 'orange.png'
        on_release: 
            app.sm.current = 'sendfld'
            root.dismiss()

    Button:
        text: '--------------------------'
        color: 1, .5, 0, 1
        size_hint_y: None
        height: 48
        background_down: 'orange.png'
        on_release: 
            root.test()
            root.dismiss()

    Button:
        text: 'Inventory Stats'
