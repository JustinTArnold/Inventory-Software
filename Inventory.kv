            root.dismiss()

<MainScreen>
	name: "MainMenu"
    btn: btn

    canvas.before:
        Color:
            rgba: 1, 1, 1, .8
        Rectangle:
            pos: self.pos
            size: self.size

	FloatLayout:
        Label:
            text:"Home"
            font_size: (root.width**2 + root.height**2) / 15**4
            pos_hint: {"x":0.35, "top":1.035}
            size_hint: 0.5, 0.15
            color: 0,0,1,1
            underline: True
            bold: True
        Button:
            id: btn
            text: 'Menu'
            on_release: 
                root.open_menu(self)
            pos_hint:{"x":0.001,"y":0.92}
            size_hint: 0.2, 0.075
            font_size: (root.width**2 + root.height**2) / 17**4
            background_normal: ''
            background_color: (0.0, 0.0, 1.0, 1.0)


        Image:
            source: 'line.png'
            size: self.texture_size
            pos_hint: {'right':.705, 'top':1}
            allow_stretch: True
            keep_ratio: True

          

